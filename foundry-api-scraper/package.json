{
  "name": "foundry-vtt-api-scraper",
  "version": "1.0.0",
  "description": "A web scraper for Foundry VTT API documentation",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "scrape": "node src/scraper.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "build": "npm run lint && npm test",
    "dev": "nodemon src/index.js",
    "toc": "node scripts/generate-toc.js",
    "index": "node scripts/generate-index.js",
    "setup": "node scripts/setup.js"
  },
  "keywords": [
    "foundry-vtt",
    "api",
    "scraper",
    "documentation",
    "offline"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "colors": "^1.4.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.1.1",
    "highlight.js": "^11.9.0",
    "jsdom": "^22.1.0",
    "marked": "^9.1.6",
    "ora": "^7.0.1",
    "progress": "^2.0.3",
    "puppeteer": "^21.11.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/foundry-vtt-api-scraper.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/foundry-vtt-api-scraper/issues"
  },
  "homepage": "https://github.com/yourusername/foundry-vtt-api-scraper#readme"
}
